    "./navigation": {
      "types": "./dist/stubs/navigation.d.ts",
      "import": "./dist/stubs/navigation.js"
    },
    "./stores": {
      "types": "./dist/stubs/stores.d.ts",
      "import": "./dist/stubs/stores.js"
    },
    "./router": {
      "types": "./dist/stubs/router.d.ts",
      "import": "./dist/stubs/router.js"
    },
    "./storage": {
      "types": "./dist/stubs/storage.d.ts",
      "import": "./dist/stubs/storage.js"
    },
    "./animate": {
      "types": "./dist/stubs/animate.d.ts",
      "import": "./dist/stubs/animate.js"
    },
    "./network": {
      "types": "./dist/stubs/network.d.ts",
      "import": "./dist/stubs/network.js"
    }
